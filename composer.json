{
	"name": "bible-exchange/bible-experience",
	"description": "The Learning Record System for Bible exchange.",
	"authors": [
        {
            "name": "Stephen Reynolds",
            "email": "sgrjr@deliverance.me"
        }
    ],
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=5.5.9",
        	"laravel/framework": "5.2.*",
		"webonyx/graphql-php": "^0.7.0",
        	"laracasts/presenter": "dev-master",
		"tymon/jwt-auth": "^0.5.9",
	 	"barryvdh/laravel-cors": "^0.8.0",
		"learninglocker/statementfactory": "0.0.x",
		"seld/jsonlint": "^1.4",
		"bshaffer/oauth2-server-php": "^1.8",
		"bshaffer/oauth2-server-httpfoundation-bridge": "^1.1",
		"itsgoingd/clockwork": "~1.10",
		"laravelcollective/html": "5.2.*",
		"graham-campbell/markdown": "^6.1",
		"auth0/login": "~3.1",
		"ivome/graphql-relay-php": "^0.2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8",
		"phpspec/phpspec": "~2.1",
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "0.9.*",
		"symfony/css-selector": "2.8.*|3.0.*",
		"symfony/dom-crawler": "2.8.*|3.0.*"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"BibleExperience\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
            	  "tests/TestCase.php"
		]
	},
    "scripts": {
		"post-update-cmd": [
		  "php artisan clear-compiled"
		]
	  },
    "config": {
		"secure-http": false,
		"minimum-stability": "dev-master",
		"prefer-stable": true
    }

}
